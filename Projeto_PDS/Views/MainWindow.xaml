<Window x:Class="Projeto_PDS.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projeto_PDS.Views"
        mc:Ignorable="d"
        WindowState="Maximized"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0">
            <Button Content="Fornecedor" Click="Button_Click" Name="MN_Fornecedor" />
            <Button Content="Cliente" Click="Button_Click" Name="MN_Cliente"/>
            <Button Content="Gastos" Click="Button_Click" Name="MN_Despesa"/>
            <Button Content="Colaboradores" Click="Button_Click" Name="MN_Funcionario"/>
            <Button Content="Produtos" Click="Button_Click" Name="MN_Produto"/>
            <Button Content="Relatórios" Click="Button_Click" Name="MN_Relatorio"/>
           
            

        </StackPanel>
        <Frame x:Name="framePage" Grid.Column="1" HorizontalAlignment="Stretch" NavigationUIVisibility="Hidden"/>




    </Grid>
</Window>
